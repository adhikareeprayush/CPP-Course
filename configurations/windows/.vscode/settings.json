{
    "files.associations": {
        "format": "cpp",
        "iostream": "cpp"
    },
    "code-runner.executorMap": {
        "cpp": "g++ -std=c++20 -o \"$fileNameWithoutExt.exe\" \"$fullFileName\" -lstdc++; if ($?) {./\"$fileNameWithoutExt.exe\"}"
    },
    "C_Cpp.errorSquiggles": "disabled"
}
